<div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>#Id</th>
              <th>Email</th>
              <th>Role</th>
              <th>Registrado</th>
            </tr>
          </thead>
          <tbody>

          <% @users.each do |user|%>
            <tr>
              <td><%=user.id%></td>
              <td><%=user.email%></td>
              <td><%=user.role%></td>
              <td><%=user.created_at%></td>
            </tr>
          <%end %>


           
          </tbody>
        </table>
      </div>